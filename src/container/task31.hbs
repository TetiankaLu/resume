
<main>
    <div>
        {{#with navigation}}
        <div>
           {{#each links }}
          <a href="{{href}}">{{text}}</a>
            {{/each}}
                {{#with button}}
                <a href="{{href}}" class="btn btn-primary btn-sm">{{text}}</a>
                {{/with}}
        </div>
       {{/with}}
    </div>
    <hr>
    <br>
    <div>
        {{#with header}}
             <h1>{{title}}</h1>
             <p>{{description}}</p>
                {{#with button}}
                <a href="{{link}}" class="btn btn-primary">{{text}}</a>
                {{/with}}
            {{/with}}
      
    </div>
    <br>
    <hr>
    <br>
<div>
    {{#with city}}
    <h2>{{title}}</h2>
    <div class="row g-2">
        {{#each cards}}
          <section class="col-5">
            {{#with picture}}
<div class="card">
    <img class="card-img-top" src="https://picsum.photos/400/200" alt="Product photo">
    {{/with}}
  <div class="card-body">
    <h5 class="card-title">{{title}}</h5>
    <p>{{description}}</p>
    {{#with button}}
    <a href="{{href}}" class="btn btn-primary">{{text}}</a>
    {{/with}}
  </div>
</div>
</section>
        {{/each}}
    </div>
    {{/with}}
</div>
<br>
<hr>
<br>
<div>
    {{#with winner}}
    <h2>{{title}}</h2>
    {{#with table}}
<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>name</th>
            <th>surname</th>
            <th>nickname</th>

        </tr>
    </thead>
    <tbody>
        {{#each user}}
            <tr>
                <td>{{place}}</td>
                <td>{{name}}</td>
                <td>{{surname}}</td>
                <td>{{nickname}}</td>
                
            </tr>
{{/each}}
    </tbody>
</table>
    {{/with}}
    {{/with}}
</div>

<br>
<hr>
<br>

<div>
    {{#with gallery}}
    <h2>{{title}}</h2>
    <div class="row g-3">
        {{#each cards}}
          <section class="col-4">
<div class="card">
  <div class="card-body">
    <h5 class="card-header">{{name}}</h5>
        <ul class="list-group-flush" >
        <li class="list-group-item">{{items}}</li>
    </ul>

  </div>
</div>
</section>
        {{/each}}
    </div>
    {{/with}}
</div>
<br>
<hr>
<br>
<div>
    {{#with relaxion}}
    <p class="display-3">{{title}}</p>
    <section class="row g-2">
    {{#each cards}}
    <section class="col-5">
<div class="card text-bg-secondary">
    <div class="card-header">{{header}}</div>
    <div class="card-body">
        <h6 class="card-title">{{title}}</h6>
        <p>{{description}}</p>
    </div>
</div>
</section>
{{/each}}
    </section>
{{/with}}
</div>
<br>
<hr>
<br>

<div>
    {{#with rest}}
    <div class="row g-2">
    {{#each cards}}
    <section class="col-6">
        <div class="card">
    <div class="card-header text-center">{{header}}</div>
    <div class="card-body text-center">
        <h6 class="card-title">{{title}}</h6>
        <p>{{description}}</p>
        <button class="btn btn-light">{{button}}</button>
    </div>
    <div class="card-footer text-center">{{footer}}</div>
</section>
{{/each}}
    </div>
    {{/with}}
    </div>


    <br>
<hr>
<br>
<div>
    {{#with proposition}}
    <p class="display-2">{{title}}</p>
    <section class="row g-2">
    {{#each card}}
    <section class="col-5">
<div class="card text-bg-warning">
    <div class="card-header">{{header}}</div>
    <div class="card-body">
        <h6 class="card-title">{{title}}</h6>
        <p>{{description}}</p>
    </div>
</div>
</section>
{{/each}}
    </section>
{{/with}}
</div>
<br>
<hr>
<br>

<div class="row g-3">
        {{#with footer}}
        <h5>{{title}}</h5>
        <div class="col-3">
        <ul class="list-group-flush">
          {{#each navigation}}
         <li class="list-group-item">
          <p href="{{href}}">{{text}}</p>
         </li>
          {{/each}}
          </div>
         <div class="col-3">
          {{#each socialMedia}}
         <li class="list-group-item">
          <p href="{{link}}" class="btn btn-dark btn-sm">{{name}}</p>
         </li>
          {{/each}}
          </div>
          <div class="col-3">
          {{#each partners}}
         <li class="list-group-item">
          <p href="{{link}}" class="btn btn-info btn-sm">{{name}}</p>
         </li>
          {{/each}}
          </div>


        </ul>

        
       {{/with}}
</div>


</main>
