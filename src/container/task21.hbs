<div class="card">
  <div class="card-body">
    {{#with heading}}
    <div class="card">
      <div class="card-body">
    {{#with main}}
      <h1>{{title}}</h1>
      <p>{{text}}</p>

      {{#with button_text}}
       <button class="btn btn-primary">{{solid}}</button>
       <br>
       <br>
       <button class="btn btn-outline-primary">{{outline}}</button>
      {{/with}}
    {{/with}}
    </div>
    </div>


        <div class="card">
      <div class="card-body">

    {{#with second}}
      <h2>{{title}}</h2>
      <p>{{text}}</p>

      {{#with button_text}}
       <button class="btn btn-secondary">{{solid}}</button>
       <br>
       <br>
       <button class="btn btn-outline-secondary">{{outline}}</button>
      {{/with}}
    {{/with}}
        </div>
    </div>

              <div class="card">
      <div class="card-body">

        {{#with subblock}}
      <h3>{{title}}</h3>
      <p>{{text}}</p>

      {{#with button_text}}
       <button class="btn btn-dark">{{solid}}</button>
       <br>
       <br>
       <button class="btn btn-outline-dark">{{outline}}</button>
      {{/with}}
    {{/with}}
        </div>
    </div>
    {{/with}}
  </div>
</div>

<div>
  <main>
   {{#with nested_display}} 
   <div class="display-1">{{title}}</div>
   <br>
   {{#with children_display}}
    <div class="display-4">{{title}}</div>
    <p>{{text}}</p>
    <br>
    {{#with button}}
     <button class="btn dtn-warning">{{text}}</button>
    {{/with}}
    {{/with}}
    <hr>
    <br>
    <div>
      <h6>{{header_text}}</h6>
      {{#with button}}
       <button class="btn btn-info">{{text}}</button>
      {{/with}}
    </div>
   {{/with}}
  </main>
</div>
<hr>

<div class="card">
  <div class="card-body">

    {{#with paragraph}}
     <p class="text-capitolize">{{title}}</p>
     <p class="text-lowercase">{{text}}</p>

     {{#with button}}
      <button class="btn btn-link">{{text}} {{url}}</button>
     {{/with}}
       <ul>
     {{#each list_name}}
          <li><button class="btn btn-link">{{text}} {{url}}</button></li>
     {{/each}}
      </ul>

    {{/with}}

  </div>
</div>